{
    "name": "flow_frontend",
    "version": "1.0.0",
    "description": "Frontend for otto flow",
    "private": true,
    "scripts": {
        "build": "npm run clean-dist && webpack -p",
        "start": "webpack-dev-server --hot",
        "format": "prettier --config .prettierrc.yaml --write '{src,configs,tests}/**/*.{ts,tsx,js,jsx,html}'",
        "format:check": "prettier --config .prettierrc.yaml --list-different '{src,configs,tests}/**/*.{ts,tsx,js,jsx,html}'",
        "lint": "tslint '{src,configs,tests}/**/*.{ts,tsx,js,jsx}'",
        "test": "jest --coverage",
        "test:watch": "jest --watch --coverage",
        "clean-dist": "rm -rfd dist"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/react-router-dom": "^5.1.3",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-hot-loader": "^4.12.18",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.4"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-decorators": "^7.7.4",
        "@babel/polyfill": "^7.7.0",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "@babel/preset-typescript": "^7.7.4",
        "@types/enzyme": "^3.10.3",
        "@types/jest": "^24.0.23",
        "@types/react": "^16.9.13",
        "@types/react-dom": "^16.9.4",
        "@types/react-redux": "^7.1.5",
        "@types/react-test-renderer": "^16.9.1",
        "babel-loader": "^8.0.6",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "enzyme-to-json": "^3.4.3",
        "fork-ts-checker-webpack-plugin": "^3.1.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jest-enzyme": "^7.1.2",
        "jest-serializer-html": "^7.0.0",
        "prettier": "^1.19.1",
        "ts-jest": "^24.2.0",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.7.2",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0"
    }
}
