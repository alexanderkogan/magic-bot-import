{
    "name": "magic-bot-import",
    "version": "1.0.0",
    "description": "Importers for the magic-bot",
    "private": true,
    "scripts": {
        "build": "npm run clean-dist && webpack -p",
        "start": "webpack-dev-server --hot",
        "format": "prettier --config .prettierrc.yaml --write '{src,configs,test}/**/*.{ts,tsx,js,jsx,html}'",
        "format:check": "prettier --config .prettierrc.yaml --list-different '{src,configs,test}/**/*.{ts,tsx,js,jsx,html}'",
        "lint": "tslint '{src,configs,test}/**/*.{ts,tsx,js,jsx}'",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "clean-dist": "rm -rfd dist"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/react-router-dom": "^5.1.3",
        "cross-fetch": "^3.0.4",
        "immer": "^8.0.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-hot-loader": "^4.12.19",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.5",
        "scryfall-sdk": "^1.5.1"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/polyfill": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@types/enzyme": "^3.10.4",
        "@types/jest": "^24.9.1",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/react-redux": "^7.1.7",
        "@types/react-test-renderer": "^16.9.2",
        "babel-loader": "^8.0.6",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.4.3",
        "fork-ts-checker-webpack-plugin": "^3.1.1",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jest-enzyme": "^7.1.2",
        "jest-serializer-html": "^7.0.0",
        "prettier": "^1.19.1",
        "ts-jest": "^24.3.0",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.7.5",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    }
}
